

<h1>Table of @Model</h1>


<table>
    <thead>
        <tr>
            <th>Number</th>
            <th>Times</th>
            <th>Result</th>
        </tr>
    </thead>
    <tbody>
        @for(int i = 1; i <= 10; i++)
        {
            <tr>
                <td>@Model</td>
                <td>@i</td>
                <td>@(Model*i)</td>
            </tr>
        }
    </tbody>
    
</table>



